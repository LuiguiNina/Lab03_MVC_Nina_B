@model Lab03_MVC_Nina_B.Models.ClsTabla
@{

}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GenerarTabla</title>
</head>
<body>
    <div> 
        <center>
            <h1>Tabla de Multiplicar</h1>
            <br />
            @for(int i = 0; i < 12; i++) {
                var m = 0;

                m = Convert.ToInt32(Model.numero * i);
                <div>@Model.numero X @i = @m  </div>
            }
        </center>
    </div>
</body>
</html>
